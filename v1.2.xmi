<?xml version="1.0" encoding="ASCII"?>
<openapi:Root
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:openapi="http://som.uoc.edu/openapi/2.0.0">
  <api swagger="2.0"
      host="virtserver.swaggerhub.com"
      basePath="/thingsboard/2.0.0">
    <info title="Thingsboard API"
        description="This is a thingsboard API"
        version="1.0.0">
      <contact
          email="you@your-company.com"/>
      <license
          name="Apache 2.0"
          url="http://www.apache.org/licenses/LICENSE-2.0.html"/>
    </info>
    <schemes>https</schemes>
    <paths relativePath="/settings">
      <post
          summary="save a new setting"
          description="save a new setting"
          responses="//@responses.0">
        <tagReferences>admin</tagReferences>
        <consumes>application/javascript</consumes>
        <produces>application/json</produces>
      </post>
    </paths>
    <paths relativePath="/User">
      <post
          parameters="//@paramters.0"
          summary="save">
        <produces>json</produces>
        <schemes>http</schemes>
        <schemes>https</schemes>
      </post>
    </paths>
    <paths relativePath="/ConfigByUser">
      <post
          parameters="//@paramters.0"
          responses="//@responses.0">
        <schemes>http</schemes>
        <schemes>https</schemes>
      </post>
    </paths>
  </api>
  <paramters
      schema="//@schemas.0"
      name="user"
      required="true"/>
  <schemas type="object"
      pattern=""
      name="User"
      title="User"
      properties="//@schemas.1 //@schemas.2 //@schemas.3"/>
  <schemas type="string"
      name="name"
      title="name"/>
  <schemas type="string"
      name="lastname"
      title="lastname"/>
  <schemas type="integer"
      name="age"
      title="age"/>
  <responses
      schema="//@schemas.0"
      description="OK"
      code="200"/>
</openapi:Root>
